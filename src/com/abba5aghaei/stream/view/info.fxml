<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="box" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="image" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
            </HBox.margin></ImageView>
        <Label fx:id="label1" text="Label">
            <HBox.margin>
                <Insets top="2.0" />
            </HBox.margin></Label>
        <Label fx:id="label2" text="Label" textFill="#8a8787">
            <HBox.margin>
                <Insets left="2.0" />
            </HBox.margin>
        </Label>
    </children>
</HBox>
